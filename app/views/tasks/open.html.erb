<% content_for :head do %>
	<% if(session[:user_student] == true) %>
		<%= render :partial => "shared/mainHeaderStudent", :locals => { :selected => "open"} %>
	<% else %>
		<%= render :partial => "shared/mainHeaderTeacher", :locals => { :selected => "open"} %>
	<% end %>
<% end %>
<% @status = :open %>

<% if(session[:user_student] == true) %>
  	<%= render 'listStudent' %>
  <% else %>
  	<%= render 'listTeacher' %>
<% end %>

<% content_for :buttons do %>
<br>
<% end %>
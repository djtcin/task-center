<h1>Listing submissions</h1>

<table>
  <tr>
    <th>Lastupdate</th>
    <th>Comment</th>
    <th>Grade</th>
    <th>Submitted</th>
    <th>Studentid</th>
    <th>Remember</th>
    <th>Viewed</th>
    <th>Task</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @submissions.each do |submission| %>
  <tr>
    <td><%= submission.lastUpdate %></td>
    <td><%= submission.comment %></td>
    <td><%= submission.grade %></td>
    <td><%= submission.submitted %></td>
    <td><%= submission.studentId %></td>
    <td><%= submission.remember %></td>
    <td><%= submission.viewed %></td>
    <td><%= submission.task %></td>
    <td><%= link_to 'Show', submission %></td>
    <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
    <td><%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Submission', new_submission_path %>
